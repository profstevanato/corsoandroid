<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Smile">
        <attr name="smileColor" format="enum">
            <enum name="white" value="0"/>
            <enum name="yellow" value="1"/>
            <enum name="green" value="2"/>
            <enum name="red" value="3"/>
        </attr>
        <attr name="smileScale" format="integer" />
    </declare-styleable>
</resources>